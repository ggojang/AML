  <term_definitions id="en">
   <items id="id1">
    <text>Examination Findings  - Pupils</text>
    <description>Findings on physical examination of the pupils.</description>
   </items>
   <items id="id3">
    <text>Measured Size</text>
    <description>Measured size of the pupil.</description>
   </items>
   <items id="id4">
    <text>Direct Light Reflex</text>
    <description>Pupil constricts with light shone into that eye.</description>
   </items>
   <items id="id5">
    <text>Consensual Light Reflex</text>
    <description>Findings of the consensual pupillary light reflex.</description>
   </items>
   <items id="id6">
    <text>Shape</text>
    <description>The shape of the pupil.</description>
   </items>
   <items id="id7">
    <text>Accommodation Reflex</text>
    <description>Findings of the pupillary accommodation reflex.</description>
   </items>
   <items id="id16">
    <text>Pupil Examined</text>
    <description>Identification of the pupil under examination.</description>
   </items>
