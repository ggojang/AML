   <items id="id1">
    <text>Macroscopic findings - Lung cancer</text>
    <description>Macroscopic anatomical pathology findings related to lung cancer.</description>
   </items>
   <items id="id2">
    <text>Tumour dimensions</text>
    <description>Details of maximum dimensions of the tumour.</description>
   </items>
   <items id="id3">
    <text>Number of tumours</text>
    <description>The overall number of tumours.</description>
   </items>
   <items id="id4">
    <text>Tumour site</text>
    <description>The site of the tumour.</description>
   </items>
   <items id="id5">
    <text>Direct spread of tumour</text>
    <description>Details of direct spread of the tumour.</description>
   </items>
   <items id="id6">
    <text>Tumour resection margin </text>
    <description>Details of medial or bronchial resection margins.</description>
   </items>
   <items id="id7">
    <text>Lymph node involvement</text>
    <description>Details of lymph node involvement by tumour.</description>
   </items>
   <items id="id8">
    <text>Non-neoplastic lung findings</text>
    <description>Findings of non-neoplastic lung pathology.</description>
   </items>
