 <terminology original_language="ISO_639-1::en">
  <term_definitions id="en">
   <items id="id1">
    <text>Body fluid or substance</text>
    <description>Details of the findings relating to a bodily fluid, secretion or substance.</description>
   </items>
   <items id="id2">
    <text>Substance</text>
    <description>The fluid or substance.</description>
   </items>
   <items id="id3">
    <text>Source</text>
    <description>The source of the fluid or substance.</description>
   </items>
   <items id="id4">
    <text>Normal statements</text>
    <description>A statement about the normality of the fluid or substance.</description>
   </items>
   <items id="id6">
    <text>Clinical description</text>
    <description>A description of the fluid or substance.</description>
   </items>
   <items id="id7">
    <text>Quantity</text>
    <description>Statements about the quantity of fluid or secretion.</description>
   </items>
   <items id="id8">
    <text>Volume</text>
    <description>The volume of the fluid or secretion.</description>
   </items>
   <items id="id9">
    <text>Change in amount</text>
    <description>Statement about the change in volume of the fluid or secretion.</description>
   </items>
   <items id="at10">
    <text>Increasing</text>
    <description>Increasing volumes of fluid.</description>
   </items>
   <items id="at11">
    <text>Stable</text>
    <description>The volume of fluid or liquid is stable.</description>
   </items>
   <items id="at12">
    <text>Decreasing</text>
    <description>The volume of fluid is decreasing.</description>
   </items>
   <items id="at13">
    <text>Ceased</text>
    <description>The quantity of body fluid is now zero.</description>
   </items>
   <items id="id14">
    <text>Colour</text>
    <description>The colour of the fluid.</description>
   </items>
   <items id="id15">
    <text>Turbidity</text>
    <description>The degree of solid material suspended in the fluid.</description>
   </items>
   <items id="at16">
    <text>Clear</text>
    <description>The fluid or secretion is crystal clear.</description>
   </items>
   <items id="at17">
    <text>Slightly cloudy</text>
    <description>There is a small amount of solid suspended in the fluid.</description>
   </items>
   <items id="at18">
    <text>Cloudy</text>
    <description>There is a large amount of solid suspended but it still transmits light.</description>
   </items>
   <items id="at19">
    <text>Opaque</text>
    <description>There is so much solid suspended that the fluid does not transmit light.</description>
   </items>
   <items id="at20">
    <text>Flocculent</text>
    <description>The fluid is mostly composed of suspended solids.</description>
   </items>
   <items id="id21">
    <text>Consistency</text>
