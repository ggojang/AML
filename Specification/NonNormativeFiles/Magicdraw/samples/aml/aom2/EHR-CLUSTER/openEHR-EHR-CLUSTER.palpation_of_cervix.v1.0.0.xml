  <term_definitions id="en">
   <items id="id1">
    <text>Palpation of Cervix</text>
    <description>Examination of the uterine cervix by palpation.</description>
   </items>
   <items id="id2">
    <text>Clinical Description</text>
    <description>Narrative description of findings on examination of the cervix.</description>
   </items>
   <items id="id3">
    <text>Relative Position</text>
    <description>Actual cervical position in relation to the expected, central anatomical position.</description>
   </items>
   <items id="at4">
    <text>Intermediate</text>
    <description>The cervix is located centrally, in the typical anatomical position.</description>
   </items>
   <items id="at5">
    <text>Right</text>
    <description>The cervix is located in the right side of the pelvis.</description>
   </items>
   <items id="at6">
    <text>Left</text>
    <description>The cervix is located on the left side of the pelvis.</description>
   </items>
   <items id="at7">
    <text>Anterior</text>
    <description>The cervix is located anteriorly, towards the pubis.</description>
   </items>
   <items id="at8">
    <text>Posterior</text>
    <description>The cervix is located posteriorly, towards the sacrum.</description>
   </items>
   <items id="id9">
    <text>Consistency</text>
    <description>Consistency of the cervix.</description>
   </items>
   <items id="at10">
    <text>Soft</text>
    <description>Cervix feels soft.</description>
   </items>
   <items id="at11">
    <text>Firm</text>
    <description>Cervix feels firm.</description>
   </items>
   <items id="id12">
    <text>Surface</text>
    <description>Surface of the cervix.</description>
   </items>
   <items id="at13">
    <text>Regular/smooth</text>
    <description>Cervix feels smooth and regular.</description>
   </items>
   <items id="at14">
    <text>Irregular</text>
    <description>Cervix feels irregular.</description>
   </items>
   <items id="id15">
    <text>Effacement</text>
    <description>Estimation of the proportion of cervical effacement, in late pregnancy or during labour.</description>
   </items>
   <items id="id28">
    <text>Thickness</text>
    <description>Thickness of the cervix.</description>
   </items>
   <items id="at29">
    <text>Thick</text>
    <description>Cervix is thick.</description>
   </items>
   <items id="at30">
    <text>Thin</text>
    <description>Cervix is thin.</description>
   </items>
   <items id="at31">
    <text>Paper-thin</text>
    <description>Cervix is very thin.</description>
   </items>
   <items id="at32">
    <text>Oedematous</text>
    <description>Cervix is thickened and swollen.</description>
   </items>
   <items id="id33">
    <text>Application to Presenting Part</text>
    <description>Application of the cervix to the presenting part of the fetus.</description>
   </items>
   <items id="at34">
    <text>Loosely applied</text>
    <description>Cervix is not moulded to the fetal presenting part.</description>
   </items>
   <items id="at35">
    <text>Tightly applied</text>
    <description>Cervis is moulded to the fetal presenting part.</description>
   </items>
   <items id="id36">
