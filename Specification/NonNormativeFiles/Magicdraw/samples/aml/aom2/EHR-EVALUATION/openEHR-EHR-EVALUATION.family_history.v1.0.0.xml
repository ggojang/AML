 <terminology original_language="ISO_639-1::en">
  <term_definitions id="en">
   <items id="id1">
    <text>Family History</text>
    <description>Summary information about the problems and/or diagnoses found in genetic relatives.</description>
   </items>
   <items id="id3">
    <text>Summary</text>
    <description>Textual description about the problems, diagnoses, and genetic markers found in genetic relatives. This field may be used to capture unstructured family history information recorded in clinical records.</description>
   </items>
   <items id="id4">
    <text>Genetic relative</text>
    <description>Details about a specific genetic relative.</description>
   </items>
   <items id="id5">
    <text>Name</text>
    <description>Name of genetic relative.</description>
   </items>
   <items id="id6">
    <text>Date of Birth</text>
    <description>Date of birth of the genetic relative.</description>
   </items>
   <items id="id9">
    <text>Medical History</text>
    <description>Detail about problems or diagnoses for the genetic relative.</description>
   </items>
   <items id="id10">
    <text>Problem/Diagnosis</text>
    <description>Identification of the genetic relative&apos;s problem or diagnosis.</description>
   </items>
   <items id="id11">
    <text>Age at Onset</text>
    <description>Estimated or actual age of the genetic relative when the problem/diagnosis was clinically recognised.</description>
   </items>
   <items id="id12">
    <text>Age at Death</text>
    <description>Age of the genetic relative at death.</description>
   </items>
   <items id="id13">
    <text>Clinical Description</text>
    <description>Narrative description or comments about clinical aspects of the genetic relative&apos;s problem/diagnosis.</description>
   </items>
   <items id="id15">
    <text>Cause of Death?</text>
    <description>True if the problem/diagnosis was a direct cause of death in this genetic relative.</description>
   </items>
   <items id="id17">
    <text>Relationship</text>
    <description>The relationship of the genetic relative to the individual.</description>
   </items>
   <items id="id21">
    <text>Alias</text>
    <description>An alias or label to uniquely identify a genetic relative, without using a personal name.</description>
   </items>
   <items id="id23">
    <text>Marker Description</text>
    <description>Description of risk-related genetic markers identified in this genetic relative.</description>
   </items>
   <items id="id24">
    <text>Deceased?</text>
    <description>Is the genetic relative is deceased?</description>
   </items>
   <items id="id25">
    <text>Genetic Markers</text>
    <description>Detailed genetic marker information for the genetic relative.</description>
   </items>
   <items id="id27">
    <text>Last updated</text>
    <description>Date that thisFamily History Summary was last updated.</description>
   </items>
   <items id="id28">
    <text>Genetic Marker Details</text>
    <description>Details about the relative&apos;s genetic markers.</description>
   </items>
  </term_definitions>
 </terminology>
