 <terminology original_language="ISO_639-1::en">
  <term_definitions id="en">
   <items id="id1">
    <text>Exposure</text>
    <description>Exposure of the subject to a chemical, physical or biological agent within their environment that has caused, or may possibly cause in the future, a negative impact on health.</description>
   </items>
   <items id="id3">
    <text>Agent</text>
    <description>Identification of the chemical, physical or biological agent to which the subject was exposed.</description>
   </items>
   <items id="id4">
    <text>Overall Description</text>
    <description>Overall description of the exposure to the identified substance.</description>
   </items>
   <items id="id5">
    <text>Ongoing Exposure</text>
    <description>Does the subject remain exposed to the substance to a degree that could be regarded as a potential risk to health?</description>
   </items>
   <items id="id6">
    <text>Category</text>
    <description>Type of exposure.</description>
   </items>
   <items id="at7">
    <text>Environmental</text>
    <description>Exposure occurred in the environment.</description>
   </items>
   <items id="at8">
    <text>Occupational</text>
    <description>Exposure occurred in the occupational environment or workplace.</description>
   </items>
   <items id="ac1">
    <text>Category (synthesised)</text>
    <description>Type of exposure. (synthesised)</description>
   </items>
   <items id="id10">
    <text>Date Updated</text>
    <description>The date this exposure summary was last updated.</description>
   </items>
   <items id="id11">
    <text>Exposure Details</text>
    <description>Detailed information about an episode of exposure to the agent.</description>
   </items>
   <items id="id12">
    <text>Date of Onset</text>
    <description>Date of onset of exposure to the agent.</description>
   </items>
   <items id="id13">
    <text>Date Ceased</text>
    <description>Date of cessation of exposure to the agent.</description>
   </items>
   <items id="id14">
    <text>Description</text>
    <description>Detailed description of an episode of exposure to the identified substance.</description>
   </items>
