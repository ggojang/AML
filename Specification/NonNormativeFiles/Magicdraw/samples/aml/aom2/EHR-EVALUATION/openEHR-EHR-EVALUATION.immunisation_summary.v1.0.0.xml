  <term_definitions id="en">
   <items id="id1">
    <text>Immunisation Summary</text>
    <description>Summary statements about the current immunisation status for an identified infectious disease or agent.</description>
   </items>
   <items id="id3">
    <text>Infectious Disease or Agent</text>
    <description>Identification of the infectious disease or agent.</description>
   </items>
   <items id="id4">
    <text>Primary Course Status</text>
    <description>Status of the primary course of immunisation.</description>
   </items>
   <items id="at5">
    <text>Not commenced</text>
    <description>The primary course has not yet commenced.</description>
   </items>
   <items id="at6">
    <text>Incomplete</text>
    <description>The primary course was not completed.</description>
   </items>
   <items id="at7">
    <text>Complete</text>
    <description>The primary course was completed.</description>
   </items>
   <items id="at8">
    <text>Indeterminate</text>
    <description>It is not known if the primary course was completed.</description>
   </items>
   <items id="id9">
    <text>Date Primary Course Completed</text>
    <description>The date, or partial date, when the primary course of vaccination was completed.</description>
   </items>
   <items id="id10">
    <text>Date of Last Booster</text>
    <description>The date, or partial date, when the last vaccination booster was administered.</description>
   </items>
   <items id="id11">
    <text>Vaccination Status</text>
    <description>The vaccination status for the specific infectious disease or agent.</description>
   </items>
   <items id="at12">
    <text>Vaccination Not Required</text>
    <description>Vaccination is not required.</description>
   </items>
   <items id="at13">
    <text>Vaccination Required</text>
    <description>Vaccination is required.</description>
   </items>
   <items id="id15">
    <text>Next Review Due</text>
    <description>The date when a review of the immunisation summary is next due.</description>
   </items>
   <items id="id16">
    <text>Last Updated</text>
    <description>The date when the immunisation summary was last updated.</description>
   </items>
   <items id="id17">
    <text>Specific Vaccine</text>
    <description>Details about the vaccination status for a specified vaccine.</description>
   </items>
   <items id="at18">
    <text>Vaccination Not Indicated</text>
    <description>Vaccination is not indicated.</description>
   </items>
   <items id="at19">
    <text>Vaccination Declined</text>
    <description>Vaccination for this infectious disease or agent has been declined by the subject or carer.</description>
   </items>
   <items id="id20">
    <text>Overall Vaccination Status</text>
    <description>Statement about the status for all vaccines.</description>
   </items>
   <items id="id21">
    <text>Next Due</text>
    <description>Date when the next vaccination is due.</description>
   </items>
   <items id="at22">
    <text>No Vaccinations Required</text>
    <description>No vaccinations have been identified that require administration.</description>
   </items>
   <items id="at23">
    <text>Vaccinations Required</text>
    <description>Vaccinations have been identified that need to be administered.</description>
   </items>
   <items id="at24">
    <text>All Vaccinations Declined</text>
    <description>The subject, or carer, has declined all vaccination administration.</description>
   </items>
   <items id="id25">
    <text>Comment</text>
    <description>Additional narrative about a specific vaccine that is not captured in structured fields.</description>
   </items>
   <items id="id26">
    <text>Description</text>
    <description>Narrative description about the vaccination status.</description>
   </items>
